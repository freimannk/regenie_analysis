// Analysis parameters

params.step1_pgen = './data/step1_input.{pgen,psam,pvar}'
params.step2_pgen = './data/step2_input.{pgen,psam,pvar}'
params.phenotype_file = './data/phenotype_file.tsv'
params.phenotype_list = 'ENSG00000070214,ENSG00000112149,ENSG00000166750'
params.covariate_file = './data/covariate_file.tsv'
params.covariate_list = 'gender_coded,PC1,PC2,PC3,PC4,PC5,PC6,PC7,PC8,PC9,PC10'
params.outdir = './'
params.prefix = 'phenotype_with_PCs'

process.executor = 'slurm'
process.queue = 'amd'

singularity {
  enabled = true
  autoMounts = true
  cacheDir = "$baseDir/singularity_img/"
}